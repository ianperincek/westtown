---
import BaseLayout from "src/layouts/BaseLayout.astro";
import FaqPage from "@components/TemplateComponents/FaqPage.astro";
import CTA from "@components/CTA.astro";
import { type CollectionEntry, getCollection } from "astro:content";
import "src/styles/services.scss";

export async function getStaticPaths() {
  // Get all entries from the 'services' collection
  const services = await getCollection("services");

  // Log to check what paths are being generated
  console.log(
    "Generated static paths:",
    services.map((service) => service.id)
  );

  return services.map((service) => ({
    params: { slug: service.id },
  }));
}

interface Props {
  entry: CollectionEntry<"services">;
}

// Fetch the slug parameter
const { slug } = Astro.params;

// Import the content for the given slug
const serviceData = await getCollection("services");
const currentService = serviceData.find((service) => service.id === slug);

if (!currentService) {
  throw new Error(`Service not found: ${slug}`);
}

const { title, description, faqItems, aboutSection, masonryLayout, cta, aboutList} =
  currentService.data;

function generateFaqSchema(faqItems) {
  return {
    "@context": "https://schema.org",
    "@type": "FAQPage",
    mainEntity: faqItems.map((faqItem) => ({
      "@type": "Question",
      name: faqItem.question,
      acceptedAnswer: {
        "@type": "Answer",
        text: faqItem.answer,
      },
    })),
  };
}

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  mainEntity: faqItems.map((faq) => ({
    "@type": "Question",
    name: faq.question,
    acceptedAnswer: {
      "@type": "Answer",
      text: faq.answer,
    },
  })),
};
const faqSchemaString = JSON.stringify(faqSchema);
